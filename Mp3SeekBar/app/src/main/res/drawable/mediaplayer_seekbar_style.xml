<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<!-- 背景图 -->
 <!-- <item
      android:id="@android:id/background"
      android:drawable="@drawable/pic_mediaplayer_seekbar_bg" />-->
    <item android:id="@android:id/background">
            <shape android:shape="rectangle" >
                <solid android:color="#4b2b16"></solid>
                <corners android:radius="10dp"></corners>
                <stroke android:color="#f9e6bc" android:width="10dp"></stroke>
            </shape>
    </item>

    <item android:id="@android:id/secondaryProgress" >
        <clip>
            <shape android:shape="rectangle" >
                <solid android:color="#4b2b16"></solid>
                <corners android:radius="10dp"></corners>
                <stroke android:color="#f9e6bc" android:width="10dp"></stroke>
            </shape>
        </clip>
    </item>

<!--  <item
      android:id="@android:id/progress"
      android:drawable="@drawable/pic_mediaplayer_seekbar_progress" />-->

    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="10dp"></corners>
                <solid android:color="#f1731a"></solid>
                <stroke android:width="10dp" android:color="#f9e6bc"></stroke>
            </shape>
        </clip>
    </item>
</layer-list>