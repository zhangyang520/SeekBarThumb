<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--背景-->
 <item android:id="@android:id/background">
     <shape android:shape="rectangle">
         <corners android:radius="@dimen/corner_radius"></corners>
         <solid android:color="@color/solid_color"></solid>
         <stroke android:color="@color/stroke_color" android:width="@dimen/stroke_width"></stroke>
     </shape>
 </item>

    <!--secondProgress-->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/corner_radius"></corners>
                <solid android:color="@color/solid_color"></solid>
                <stroke android:color="@color/stroke_color" android:width="@dimen/stroke_width"></stroke>
            </shape>
        </clip>
    </item>

    <!--progress-->
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/corner_radius"></corners>
                <solid android:color="@color/progress_color"></solid>
                <stroke android:color="@color/stroke_color" android:width="@dimen/stroke_width"></stroke>
            </shape>
        </clip>
    </item>
</layer-list>